# Environment variables for Social Leaf Backend
# Copy this file to .env and fill in your values

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key

# ============================================
# AI Services (Vision & Voice)
# ============================================

# OpenRouter (PRIMARY - FREE vision models)
# Get key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Gemini AI (FALLBACK for vision)
# Get key from: https://aistudio.google.com/apikey
GEMINI_API_KEY=

# ElevenLabs (Voice generation)
# Get key from: https://elevenlabs.io/
ELEVENLABS_API_KEY=

# ============================================
# App Configuration
# ============================================
APP_ENV=development
DEBUG=true
CORS_ORIGINS=http://localhost:5173,http://localhost:8080
FRONTEND_URL=http://localhost:8080

# ============================================
# YouTube API
# ============================================
YOUTUBE_API_KEY=

# ============================================
# OAuth Credentials (Optional)
# ============================================

# YouTube (Google Cloud Console)
YOUTUBE_CLIENT_ID=
YOUTUBE_CLIENT_SECRET=

# Instagram (Facebook Developers)
INSTAGRAM_APP_ID=
INSTAGRAM_APP_SECRET=

# Twitter (Twitter Developer Portal)
TWITTER_CLIENT_ID=
TWITTER_CLIENT_SECRET=

# LinkedIn (LinkedIn Developers)
LINKEDIN_CLIENT_ID=
LINKEDIN_CLIENT_SECRET=
